<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="gvn.project311.coffeeS">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="gvn.project311.coffeeS.Example.Application.coffeeSApplication"
        android:allowBackup="true"
        android:icon="@drawable/coffee_icon_temp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coffee_icon_temp"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="gvn.project311.coffeeS.Example.Activity.MainActivity"
            android:windowSoftInputMode="stateHidden|adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="gvn.project311.coffeeS.Example.Activity.PermissionActivity"></activity>
    </application>

</manifest>